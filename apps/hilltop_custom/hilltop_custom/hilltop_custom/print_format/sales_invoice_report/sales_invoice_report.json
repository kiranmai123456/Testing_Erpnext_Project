 "css": ".columns {\n    display: flex;\n}\n.column {\n    flex: 50%;\n    border :1px solid black;    \n    }\n    \n    \ntable {\n    border-collapse: collapse;\n    width: 100%;\n    }\nth, td {\n    border: 1px solid #dddddd;\n    text-align: left;\n    padding: 8px;\n    }",
